[Desktop Entry]
_Name=Transmission
_GenericName=BitTorrent Client
_X-GNOME-FullName=Transmission BitTorrent Client
_Comment=Download and share files over BitTorrent
Exec=leechmission-gtk %U
Icon=leechmission
Terminal=false
TryExec=leechmission-gtk
Type=Application
StartupNotify=true
MimeType=application/x-bittorrent;x-scheme-handler/magnet;
Categories=Network;FileTransfer;P2P;GTK;
X-Ubuntu-Gettext-Domain=leechmission
X-AppInstall-Keywords=torrent 
Actions=Pause;Minimize;

[Desktop Action Pause]
_Name=Start Transmission with All Torrents Paused
Exec=leechmission-gtk --paused

[Desktop Action Minimize]
_Name=Start Transmission Minimized
Exec=leechmission-gtk --minimized
